<launch>
<!-- Start tilt joint controller -->
    <rosparam file="$(find my_dynamixel_tutorial)/yaml/motors.yaml" command="load"/>
    <node name="controller_spawner" pkg="dynamixel_controllers" type="controller_spawner.py"
          args="--manager=mcm
                --port motor_my
                first_motor
                second_motor  
                "
          output="screen"/>
</launch>
